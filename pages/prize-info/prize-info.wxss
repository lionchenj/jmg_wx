}
.prize-info-list {
  height: 88rpx;
  padding: 0 32rpx;
  line-height: 88rpx;
  font-size: 34rpx;
  background: #fff;
  border-bottom: 1rpx solid #e3e3e3;
}

.prize-info-list input {
  line-height: 88rpx;
  height: 88rpx;
  text-align: right;
  padding-right: 32rpx;
  float: right;
  color: #888;
}

.prize-info-list text {
  float: right;
  color: #888;
}
.prize-info-text{
  font-size: 28rpx;
  padding: 20rpx 32rpx;
  color: #363A3F;
}
